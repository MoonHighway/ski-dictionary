{
  "name": "ski-dictionary",
  "version": "1.0.0",
  "description": "A dictionary of skier terms used to teach Node and Express",
  "main": "app-server.js",
  "author": "Alex Banks <alex@moonhighway.com> (http://www.moonhighway.com)",
  "license": "MIT",
  "scripts": {
    "prestart": "grunt",
    "start": "node app-server.js",
    "stop": "killall -9 skidictionary",
    "pretest": "grunt",
    "test": "export NODE_ENV=test & ./node_modules/.bin/istanbul cover ./node_modules/.bin/_mocha",
    "posttest": "export NODE_ENV=development",
    "predev": "grunt",
    "dev": "open http://localhost:3000 & node-dev app-server & grunt watch",
    "predebug": "grunt",
    "debug": "open http://localhost:3000 & open http://localhost:8080/debug?port=5858",
    "postdebug": "node-inspector & node --debug app-server.js"
  },
  "dependencies": {
    "body-parser": "^1.14.1",
    "cors": "^2.7.1",
    "express": "^4.13.3",
    "jquery": "^2.1.4"
  },
  "devDependencies": {
    "browserify": "^11.2.0",
    "chai": "^3.3.0",
    "grunt": "^0.4.5",
    "grunt-autoprefixer": "^3.0.3",
    "grunt-browserify": "^4.0.1",
    "grunt-contrib-jshint": "^0.11.3",
    "grunt-contrib-less": "^1.0.1",
    "grunt-contrib-watch": "^0.6.1",
    "istanbul": "^0.4.0",
    "mocha": "^2.3.3",
    "rewire": "^2.3.4",
    "supertest": "^1.1.0"
  }
}
